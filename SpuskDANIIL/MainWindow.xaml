<Window x:Class="SpuskDANIIL.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SpuskDANIIL"
        mc:Ignorable="d"
        Title="MainWindow" Height="636" Width="685">
    <Grid Margin="0,0,0,24">
        <DataGrid x:Name="dgMethod" IsReadOnly="True" AutoGenerateColumns="False" Visibility="Visible" Margin="10" SelectionChanged="dgMethod_SelectionChanged">
            <DataGrid.Columns>
                <DataGridTextColumn Header="K" Binding="{Binding Path=k}" />
                <DataGridTextColumn Header="X1" Binding="{Binding Path=x1}" />
                <DataGridTextColumn Header="X2" Binding="{Binding Path=x2}" />
                <DataGridTextColumn Header="dx1" Binding="{Binding Path=dx1}" />
                <DataGridTextColumn Header="dx2" Binding="{Binding Path=dx2}" />
                <DataGridTextColumn Header="lambda" Binding="{Binding Path=lambda}" />
                <DataGridTextColumn Header="Func(A)" Binding="{Binding Path=fA}" />
                <DataGridTextColumn Header="cosA" Binding="{Binding Path=cosa}" />
                <DataGridTextColumn Header="alpha" Binding="{Binding Path=alpha}" />
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>
